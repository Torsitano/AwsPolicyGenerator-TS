{
    "game": {
        "resource": "game",
        "arn": "arn:${Partition}:gamesparks:${Region}:${Account}:game/${GameId}",
        "conditionKeys": [
            "aws:ResourceTag/${TagKey}"
        ]
    },
    "stage": {
        "resource": "stage",
        "arn": "arn:${Partition}:gamesparks:${Region}:${Account}:game/${GameId}/stage/${StageName}",
        "conditionKeys": [
            "aws:ResourceTag/${TagKey}"
        ]
    }
}