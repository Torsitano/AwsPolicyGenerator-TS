{
    "canary": {
        "resourceName": "canary",
        "service": "synthetics",
        "resourceArn": "arn:${Partition}:synthetics:${Region}:${Account}:canary:${CanaryName}",
        "resourceConditions": {
            "aws:ResourceTag/${TagKey}": {
                "condition": "aws:ResourceTag/${TagKey}",
                "description": "Filters access based on the tags associated with the resource",
                "type": "String"
            }
        },
        "listPrivileges": {},
        "readPrivileges": {
            "GetCanary": {
                "privilege": "GetCanary",
                "description": "Grants permission to get a canary details",
                "accessLevel": "Read",
                "apiDocumentationLink": "https://docs.aws.amazon.com/AmazonSynthetics/latest/APIReference/API_GetCanary.html",
                "privConditions": {},
                "dependentActions": [],
                "required": true
            },
            "GetCanaryRuns": {
                "privilege": "GetCanaryRuns",
                "description": "Grants permission to list information about all the test runs associated with a canary",
                "accessLevel": "Read",
                "apiDocumentationLink": "https://docs.aws.amazon.com/AmazonSynthetics/latest/APIReference/API_GetCanaryRuns.html",
                "privConditions": {},
                "dependentActions": [],
                "required": true
            },
            "ListTagsForResource": {
                "privilege": "ListTagsForResource",
                "description": "Grants permission to list all tags and values associated with a canary",
                "accessLevel": "Read",
                "apiDocumentationLink": "https://docs.aws.amazon.com/AmazonSynthetics/latest/APIReference/API_ListTagsForResource.html",
                "privConditions": {},
                "dependentActions": [],
                "required": false
            }
        },
        "writePrivileges": {
            "DeleteCanary": {
                "privilege": "DeleteCanary",
                "description": "Grants permission to delete a canary. Amazon Synthetics deletes all the resources except for the Lambda function and the CloudWatch Alarms if you created one",
                "accessLevel": "Write",
                "apiDocumentationLink": "https://docs.aws.amazon.com/AmazonSynthetics/latest/APIReference/API_DeleteCanary.html",
                "privConditions": {},
                "dependentActions": [],
                "required": true
            },
            "StartCanary": {
                "privilege": "StartCanary",
                "description": "Grants permission to start a canary, so that Amazon CloudWatch Synthetics starts monitoring a website",
                "accessLevel": "Write",
                "apiDocumentationLink": "https://docs.aws.amazon.com/AmazonSynthetics/latest/APIReference/API_StartCanary.html",
                "privConditions": {},
                "dependentActions": [],
                "required": true
            },
            "StopCanary": {
                "privilege": "StopCanary",
                "description": "Grants permission to stop a canary",
                "accessLevel": "Write",
                "apiDocumentationLink": "https://docs.aws.amazon.com/AmazonSynthetics/latest/APIReference/API_StopCanary.html",
                "privConditions": {},
                "dependentActions": [],
                "required": true
            },
            "UpdateCanary": {
                "privilege": "UpdateCanary",
                "description": "Grants permission to update a canary",
                "accessLevel": "Write",
                "apiDocumentationLink": "https://docs.aws.amazon.com/AmazonSynthetics/latest/APIReference/API_UpdateCanary.html",
                "privConditions": {},
                "dependentActions": [],
                "required": true
            }
        },
        "permManPrivileges": {},
        "tagPrivileges": {
            "TagResource": {
                "privilege": "TagResource",
                "description": "Grants permission to add one or more tags to a canary",
                "accessLevel": "Tagging",
                "apiDocumentationLink": "https://docs.aws.amazon.com/AmazonSynthetics/latest/APIReference/API_TagResource.html",
                "privConditions": {},
                "dependentActions": [],
                "required": false
            },
            "UntagResource": {
                "privilege": "UntagResource",
                "description": "Grants permission to remove one or more tags from a canary",
                "accessLevel": "Tagging",
                "apiDocumentationLink": "https://docs.aws.amazon.com/AmazonSynthetics/latest/APIReference/API_UntagResource.html",
                "privConditions": {},
                "dependentActions": [],
                "required": false
            }
        }
    }
}