                "required": true
            },
            "GetProfile": {
                "privilege": "GetProfile",
                "description": "Grants permission to get aggregated profiles for a specific profiling group",
                "accessLevel": "Read",
                "apiDocumentationLink": "https://docs.aws.amazon.com/codeguru/latest/profiler-api/API_GetProfile.html",
                "privConditions": {},
                "dependentActions": [],
                "required": true
            },
            "GetRecommendations": {
                "privilege": "GetRecommendations",
                "description": "Grants permission to get recommendations",
