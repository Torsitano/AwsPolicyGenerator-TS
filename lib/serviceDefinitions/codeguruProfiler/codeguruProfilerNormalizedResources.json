                "accessLevel": "Write",
                "apiDocumentationLink": "https://docs.aws.amazon.com/codeguru/latest/profiler-api/API_RemoveNotificationChannel.html",
                "privConditions": {},
                "dependentActions": [],
                "required": true
            },
            "SubmitFeedback": {
                "privilege": "SubmitFeedback",
                "description": "Grants permission to submit user feedback for useful or non useful anomaly",
                "accessLevel": "Write",
                "apiDocumentationLink": "https://docs.aws.amazon.com/codeguru/latest/profiler-api/API_SubmitFeedback.html",
                "privConditions": {},
                "dependentActions": [],
                "required": true
            },
            "UpdateProfilingGroup": {
                "privilege": "UpdateProfilingGroup",
                "description": "Grants permission to update a specific profiling group",
                "accessLevel": "Write",
                "apiDocumentationLink": "https://docs.aws.amazon.com/codeguru/latest/profiler-api/API_UpdateProfilingGroup.html",
                "privConditions": {},
                "dependentActions": [],
                "required": true
            }
        },
        "permManPrivileges": {
            "PutPermission": {
                "privilege": "PutPermission",
                "description": "Grants permission to update the list of principals allowed for an action group in the resource policy associated with the specified Profiling Group",
