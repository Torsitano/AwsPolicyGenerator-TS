{
    "project": {
        "resource": "project",
        "arn": "arn:${Partition}:codestar:${Region}:${Account}:project/${ProjectId}",
        "conditionKeys": [
            "aws:ResourceTag/${TagKey}"
        ]
    },
    "user": {
        "resource": "user",
        "arn": "arn:${Partition}:iam::${Account}:user/${aws:username}",
        "conditionKeys": [
            "iam:ResourceTag/${TagKey}"
        ]
    }
}